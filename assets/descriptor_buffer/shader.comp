#version 450
#extension GL_EXT_buffer_reference : require
#extension GL_EXT_nonuniform_qualifier : require

layout(buffer_reference, buffer_reference_align = 16) buffer StorageBuffer {
    uint data[];
};

layout(set = 0, binding = 0) uniform StorageBuffer dataIn;
layout(set = 0, binding = 1) uniform StorageBuffer dataOut;

void main()
{
    dataOut.data[0] = dataIn.data[0];
}
